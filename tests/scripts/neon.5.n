// neon.5.n - Boolean logic and comparisons

// Expected:
// false
// true
// false
// true
// false
// true

val t = true
val f = false
print(t == f         )      // false)
print(t != f         )      // true)
print(t && f         )      // false)
print(t || f         )      // true)
print(!t             )      // false)
print(!f             )      // true)