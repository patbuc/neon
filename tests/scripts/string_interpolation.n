// Expected:
// Hello Alice
// 5 + 3 = 8
// The answer is 42
// [1, 2, 3]
// true
// 3.14
// Result: 100

// Interpolation with variable
var name = "Alice"
print "Hello ${name}"

// Multiple expressions
var x = 5
var y = 3
print "${x} + ${y} = ${x + y}"

// Interpolation with number
var answer = 42
print "The answer is ${answer}"

// Interpolation with array
var arr = [1, 2, 3]
print "${arr}"

// Interpolation with boolean
var flag = true
print "${flag}"

// Interpolation with float
var pi = 3.14
print "${pi}"

// Complex expression
var a = 10
var b = 10
print "Result: ${a * b}"
