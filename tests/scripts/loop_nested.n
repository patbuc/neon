// loop_nested.n - Nested loops with break statements

// Expected:
// 0
// 0
// 0
// 1
// 1
// 0
// 1
// 1
// 2
// 0
// 2
// 1

var outer = 0
loop {
    if (outer >= 3) {
        break
    }

    var inner = 0
    loop {
        if (inner >= 2) {
            break
        }
        print outer
        print inner
        inner = inner + 1
    }

    outer = outer + 1
}
