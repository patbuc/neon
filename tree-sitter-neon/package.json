{
  "name": "tree-sitter-neon",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for Neon programming language",
  "main": "bindings/node",
  "keywords": ["parser", "lexer", "neon", "tree-sitter"],
  "repository": {
    "type": "git",
    "url": "https://github.com/patbuc/neon.git",
    "directory": "tree-sitter-neon"
  },
  "author": "patbuc",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "scripts": {
    "test": "tree-sitter test",
    "generate": "tree-sitter generate",
    "build": "tree-sitter generate && node-gyp build",
    "parse": "tree-sitter parse"
  },
  "tree-sitter": [{
    "scope": "source.neon",
    "file-types": ["neon", "n"],
    "highlights": "queries/highlights.scm",
    "injections": "queries/injections.scm"
  }]
}
